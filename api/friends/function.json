{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "put",
        "post"
      ],
      "route": "friends/{person1}/{person2}/{inviter}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "friendEntity",
      "type": "table",
      "tableName": "friends",
      "partitionKey": "{person1}",
      "rowKey": "{person2}",
      "direction": "in",
      "connection": "TableConnection"
    },
    {
      "name": "tableOut",
      "type": "table",
      "tableName": "friends",
      "partitionKey": "friends",
      "direction": "out",
      "connection": "TableConnection"
    }
  ]
}